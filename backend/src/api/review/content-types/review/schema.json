{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "stars": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5,
      "required": true
    },
    "review_content": {
      "type": "text"
    },
    "business_details": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::business-detail.business-detail",
      "inversedBy": "reviews"
    },
    "users_permissions_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "reviews"
    },
    "review_photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
